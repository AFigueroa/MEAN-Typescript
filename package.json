{
  "name": "ng-universal-crud",
  "version": "0.0.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/angular/universal-starter.git"
  },
  "scripts": {
    "ng": "ng",
    "start": "npm run serve:ssr",
    "ssr": "npm run build:ssr && npm run serve:ssr",
    "build": "ng build",
    "rebuild": "ng run ng-universal-crud:server:production && npm run webpack:server && npm run serve:ssr",
    "lint": "ng lint ng-universal-crud",
    "build:client-and-server-bundles": "ng build --prod && ng run ng-universal-crud:server:production",
    "build:prerender": "npm run build:client-and-server-bundles && npm run webpack:server && npm run generate:prerender",
    "build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server",
    "generate:prerender": "cd dist && node prerender",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:prerender": "cd dist/browser && http-server",
    "serve:ssr": "node dist/server"
  },
  "pre-commit": [],
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.0.0",
    "@angular/common": "^6.0.0",
    "@angular/compiler": "^6.0.0",
